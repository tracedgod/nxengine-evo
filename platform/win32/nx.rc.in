#include <windows.h>

#define VER_FILEVERSION                     @nx_VERSION_MAJOR@,@nx_VERSION_MINOR@,@nx_VERSION_RELEASE@,0
#define VER_PRODUCTVERSION                  @nx_VERSION_MAJOR@,@nx_VERSION_MINOR@,@nx_VERSION_RELEASE@,0

VS_VERSION_INFO VERSIONINFO
FILEVERSION VER_FILEVERSION
PRODUCTVERSION VER_PRODUCTVERSION
FILEFLAGSMASK 0x3fL
#ifdef _DEBUG
FILEFLAGS 0x1L
#else
FILEFLAGS 0x0L
#endif
FILEOS 0x4L
FILETYPE 0x1L
FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904E4"
        BEGIN
            VALUE "CompanyName", "NXEngine Team\0"
            VALUE "FileDescription", "A somewhat upgraded/refactored version of NXEngine\0"
            VALUE "FileVersion", "@nx_VERSION_MAJOR@.@nx_VERSION_MINOR@.@nx_VERSION_RELEASE@.0\0"
            VALUE "InternalName", "@nx_PRODUCT_NAME@\0"
            VALUE "LegalCopyright", "Copyright (C) 2024 NXEngine Team\0"
            VALUE "OriginalFilename", "@CMAKE_PROJECT_NAME@\0"
            VALUE "ProductVersion", "@nx_VERSION_MAJOR@.@nx_VERSION_MINOR@.@nx_VERSION_RELEASE@.0\0"
            VALUE "ProgramID", "@nx_APP_ID@\0"
            VALUE "ProductName", "@nx_PRODUCT_NAME@\0"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x0409, 0x04E4
    END
END

IDI_MAIN_ICON ICON "@ICON_PATH@"